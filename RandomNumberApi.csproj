    <Project Sdk="Microsoft.NET.Sdk.Web">

      <PropertyGroup>
        <TargetFramework>net8.0</TargetFramework> <!-- Or net6.0, net7.0, etc. based on your SDK -->
        <Nullable>enable</Nullable>
        <ImplicitUsings>enable</ImplicitUsings>
      </PropertyGroup>

      <ItemGroup>
        <!-- Include necessary NuGet packages for Web API and Swagger -->
        <PackageReference Include="Microsoft.AspNetCore.OpenApi" Version="8.0.0" /> <!-- Adjust version based on TargetFramework -->
        <PackageReference Include="Swashbuckle.AspNetCore" Version="6.4.0" /> <!-- Adjust version -->
      </ItemGroup>

    </Project>
    